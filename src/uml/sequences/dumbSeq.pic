
.PS
copy "sequence.pic";

boxwid = 1.3;
boxht = 0.7;
movewid = 0.9;
spacing = 0.33;

# Define the objects
object(S,"s:Switch");
object(P,"p:Pump");
placeholder_object(F);
step();
active(S);
active(P);

# Message sequences
message(S,P,"run()");
create_message(P,F,"f:Flow");
active(F);
message(S,P,"stop()");
destroy_message(P,F);

# Object completion
step();
complete(S);
complete(P);

.PE
